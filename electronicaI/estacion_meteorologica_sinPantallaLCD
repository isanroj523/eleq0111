#include <DHT.h>
int dhtPin = 2;
DHT dht(dhtPin, DHT11);  

void setup(){
  Serial.begin(9600);
  dht.begin();
  delay(5000);
}

void loop()
{
  delay(5000);
  float humedad = dht.readHumidity();
  float temperatura = dht.readTemperature();

  Serial.print("Humedad: ");
  Serial.print(humedad);
  Serial.print("% ");

  Serial.print("Temperatura: ");
  Serial.print(temperatura);
  Serial.println("Â°C ");
}
